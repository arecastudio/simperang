	<queryString>
		<![CDATA[SELECT b.nama,CONCAT(SUM(p2.jml_minta)," ",b.satuan) AS jml,(b.harga)AS harga_satuan,(b.harga*SUM(p2.jml_minta))AS total_harga FROM dpb_kolektif_d AS d2 INNER JOIN permintaan_d AS p2 ON p2.nomor_permintaan=d2.nomor_permintaan INNER JOIN barang AS b ON b.id=p2.id_barang WHERE d2.nomor_dpb_kolektif=$P{parameter1} GROUP BY b.id ;]]>
	</queryString>
